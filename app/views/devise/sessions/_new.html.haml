#modal-window__title
  %h3 Log in
#modal-window__form
  = form_for(resource, as: resource_name, url: session_path(resource_name), remote: true) do |f|
    .error
    .field
      = f.label :email
      %br/
      = f.email_field :email, autofocus: true, autocomplete: "email", id: "new_user_session_email"
    .field
      = f.label :password
      %br/
      = f.password_field :password, autocomplete: "current-password", id: "new_user_session_password"
    - if devise_mapping.rememberable?
      .field
        = f.check_box :remember_me
        = f.label :remember_me
    .actions
      = f.submit "Log in", class: "btn-primary"
-# = render "devise/shared/links"
